!!
!! A 2D slice of the BEAVRS benchmark
!!
!! Use ENDF/B-VII.1 library for consistency with Boyd thesis
!! A 3.1% enriched fuel assembly with no burnable poisons
!! To achieve degernerate homogenisation, model quarter assembly to reduce number of pins to generate cross sections for. 
type eigenPhysicsPackage;

pop      1000;
inactive 150;
active   150;
XSdata mgData;
dataType mg;

// Specify output format default asciiMATLAB
outputFormat asciiJSON;
outputFile QuarterAssembly_MC_MG_output;

collisionOperator { neutronMG {type neutronMGstd;} }

transportOperator { 
                    type transportOperatorHT; cache 1;
                  }

inactiveTally {
  shannon {
	    type shannonEntropyClerk;
            map {type multiMap;
            maps (xax yax);
            xax { type spaceMap; grid lin; min -5.67; max 5.67; N 9; axis x;}
            yax { type spaceMap; grid lin; min -5.67; max 5.67; N 9; axis y;}
                }
           cycles 500;
          }
}

